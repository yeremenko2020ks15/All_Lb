<p id="notice"><%= notice %></p>

<h1>Patient Carts</h1>

<table>
  <thead>
    <tr>
      <th>Klinik</th>
      <th>Doctor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patient_carts.each do |patient_cart| %>
      <tr>
        <td><%= patient_cart.klinik_id %></td>
        <td><%= patient_cart.doctor_id %></td>
        <td><%= link_to 'Show', patient_cart %></td>
        <td><%= link_to 'Edit', edit_patient_cart_path(patient_cart) %></td>
        <td><%= link_to 'Destroy', patient_cart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient Cart', new_patient_cart_path %>
